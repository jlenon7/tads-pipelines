{
  "name": "@jlenon7/tads-pipelines-test",
  "version": "1.0.1",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "build": "tsc",
    "test:run": "jest",
    "test:lint": "eslint ./src --fix --ext=.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlenon7/tads-pipelines.git"
  },
  "author": "Jo√£o Lenon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlenon7/tads-pipelines/issues"
  },
  "homepage": "https://github.com/jlenon7/tads-pipelines#readme",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^2.4.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-adonis": "^1.0.15",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "mrm": "^2.5.13",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
